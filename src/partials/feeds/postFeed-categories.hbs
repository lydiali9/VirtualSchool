{{#each data}}
<div class="feed-column">
  <div class="row column feed-title">
    <h4 class="title">{{title}}</h4>
  </div>
  <div class="row feed-content">
    <div class="featured-img" data-imagizer>
      <img src="{{img}}" alt=""/>
    </div>
    <div class="columns expand">
      <div class="row">
        <div class="content">
          <h4 class="description-heading">OVERVIEW</h4>
          <h5 class="description">{{description}}</h5>
        </div>
        <div class="list-items">
          <h4 class="list-heading">PATHS</h4>
          <ul class="listing">
            {{#each list-items}}
              <li>{{this}}</li>
            {{/each}}
          </ul>
          <a class="link-back" href="/learningPaths/{{filename}}.html">{{link-title}}</a>
        </div>
      </div>
    </div>
  </div>
</div>
{{/each}}
